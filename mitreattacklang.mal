category Hardware {
    asset Computer {
        | infectedComputer
            ->  removableMedia1.infectedMedia,
                internalNetwork6.applicationLayerConnexion,
                os5.commonlyUsedPortsConnection,
                os5.uncommonlyUsedPortsConnection,
                //os.endpointDenialOfService,  // 2015 Ukraine power grid
                //os.fileDeletion,  // 2015 Ukraine power grid
                removableMedia1.dataFromRemovableMedia

        | infectedWindowsComputer
            ->  windows8.dynamicDataExchange
                //windows.accountManipulation,  // 2016 the Cayman National Isle of Man Bank attack
                //windows.powerShellProfile  // 2016 the Cayman National Isle of Man Bank attack
                
        | infectedLinuxComputer

        | infectedMacOSComputer 

        | unresponsive

        | exfiltrationOverPhysicalMedium
            ->  removableMedia1.dataExfiltrated

        E microphoneExists
            <-  microphone
            ->  microphone.collectAudio

        & collectAudio
            ->  os5.dataCollected
        
        | peripheralDeviceDiscovery
            user info: "Adversaries may attempt to gather information about attached peripheral devices and components connected to a computer system. The information may be used to enhance their awareness of the system and network environment or may be used for further actions."

        E webcamExists
            <-  webcam
            ->  webcam._collectVideo

        E videoCallApplicationExists
            <-  videoCallApplication
            ->  videoCallApplication._collectVideo

        | collectVideo
            ->  os5.dataCollected
    }

    asset C2Server extends Computer {
        | c2Connected 
            ->  communicate,
                dataObfuscation,
                packetCapture

        | communicate 

        & dataExfiltrated

        | generateDomainNames 

        | _dataObfuscation
            ->  dataObfuscation

        & dataObfuscation
            user info: "This encompasses many methods, such as adding junk data to protocol traffic, using steganography, commingling legitimate traffic with C2 communications traffic, or using a non-standard data encoding system."
            ->  externalNetwork3.bypassNetworkIntrusionDetection,
                internalNetwork3.bypassNetworkIntrusionDetection,
                bypassNetworkIntrusionDetection

        | packetCapture

        | bypassNetworkIntrusionDetection
    }

    asset HardwareAddition extends Computer {
        & hardwareAdditions
            user info: "Computer accessories, computers, or networking hardware may be introduced into a system as a vector to gain execution." 
            ->  os18.executeCode  
    }

    asset NTFSPartition extends Computer {
        E ntfsPartitionExists
            <-  ntfsPartition2
            ->  windows13.ntfsFileAttributes
    } 

    asset Webcam {
        | _collectVideo
            -> computer10.collectVideo
    }

    asset RemovableMedia {
        | dataFromRemovableMedia
            ->  os12.sensitiveDataCollected

        | infectedMedia

        & dataExfiltrated
    }
    
    asset Microphone {
        | collectAudio
            ->  computer8.collectAudio
    }
}

